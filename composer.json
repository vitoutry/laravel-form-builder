{
    "name": "vitoutry/laravel-form-builder",
    "description": "Laravel form builder  (make a support laravel 8, kris)",
    "keywords": ["laravel", "form", "builder","symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vitou Try",
            "email": "vitoutry@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "laravelcollective/html": "^6|^7|^8",
        "illuminate/database": "^7|^8",
        "illuminate/validation": "^7|^8"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.16.x-dev"
        },
        "laravel": {
            "providers": [
                "Vitoutry\\LaravelFormBuilder\\FormBuilderServiceProvider"
            ],
            "aliases": {
                "FormBuilder": "Vitoutry\\LaravelFormBuilder\\Facades\\FormBuilder"
            }
        }
    },
    "autoload": {
        "psr-0": {
            "vitoutry\\LaravelFormBuilder": "src/"
        },
        "classmap": [
            "tests/FormBuilderTestCase.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
